{
	"env": {
		"development": {
			"application/javascript": {
				"presets": [
					[
						"env",
						{
							"targets": {
								"electron": "2.0"
							}
						}
					],
					"react"
				],
				"plugins": [
					"transform-async-to-generator"
				],
				"sourceMaps": "inline"
			},
			"text/typescript": {
				"removeComments": false,
				"preserveConstEnums": true,
				"sourceMap": true,
				"declaration": true,
				"noImplicitAny": true,
				"noImplicitReturns": true,
				"suppressImplicitAnyIndexErrors": true,
				"strictNullChecks": true,
				"noUnusedLocals": false,
				"noImplicitThis": true,
				"noUnusedParameters": true,
				"inlineSourceMap": true,
				"inlineSources": true,
				"importHelpers": true,
				"noEmitHelpers": true,
				"module": "commonjs",
				"moduleResolution": "node",
				"pretty": true,
				"target": "es2015",
				"jsx": "react"
			}
		},
		"production": {
			"application/javascript": {
				"presets": [
					[
						"env",
						{
							"targets": {
								"electron": "2.0"
							}
						}
					],
					"react"
				],
				"plugins": [
					"transform-async-to-generator"
				],
				"sourceMaps": "none"
			},
			"text/typescript": {
				"removeComments": false,
				"preserveConstEnums": true,
				"declaration": true,
				"noImplicitAny": true,
				"noImplicitReturns": true,
				"suppressImplicitAnyIndexErrors": true,
				"strictNullChecks": true,
				"noUnusedLocals": true,
				"noImplicitThis": true,
				"noUnusedParameters": true,
				"sourceMap": false,
				"importHelpers": true,
				"noEmitHelpers": true,
				"experimentalDecorators": true,
				"target": "es2015",
				"jsx": "react"
			}
		}
	}
}
